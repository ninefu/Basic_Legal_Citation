/* CSS Document */

/* container into which the real stuff gets shoehorned */
	
	/* accessibility - for screen readers */
		a.skipNav, a.skipNav:visited {
		 POSITION: absolute;
		 LEFT: -9000px;
		 TOP: -9000px;
		}
	
	/* enormous page-containing DIVs */
		DIV#container {
		 MARGIN: 0 auto;
		 PADDING: 1px;
		 BACKGROUND: #333;
		 BACKGROUND: #68645b;
		 MIN-HEIGHT: 1200px;	
		}
		
		DIV.ours {
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		}
		
		DIV.collection {
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		 BACKGROUND: #fff;
		}
	
	/* CU  */
		#cu-head { 
		 MIN-HEIGHT: 45px;
		 HEIGHT: 3.6em;
		 WIDTH: 100%;
		 BACKGROUND: #68645b;
		}
		
		#cu-identity {
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		}
		#cu-logo {
		 FLOAT: left;
		}
		
		A>IMG {
		 BORDER: 0;
		}
		
		IMG#insig {
		 WIDTH: 54px;
		 HEIGHT: 45px;
		 BORDER: 0;
		}
		IMG#cls {
		 WIDTH: 200px;
		 HEIGHT: 45px;
		 BORDER: 0;
		}
		
		#cu-search {
		 BACKGROUND: #68645b;
		}
		#cu-search a {
		 FLOAT: right;
		 MARGIN-TOP: 5px;	
		 MARGIN-RIGHT: 5px;
		 PADDING: 10px;
		 FONT-SIZE: 0.9em;
		 COLOR: #fff;
		 TEXT-DECORATION: none;
		 BORDER: none;
		 BACKGROUND: #68645b;
		 }
		 
		#cu-search a:hover {
		 BACKGROUND: #555;
		}
	
	/* any header */
		.header {
		 CLEAR: both;
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		 PADDING: 0;
		}
	
	/* sitewide header */
	
		#lii-head {
		 MIN-HEIGHT: 2.2em;
		 HEIGHT: auto;
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		 PADDING: 0;
		 BACKGROUND: #ccc;
		}
		
		DIV.liiround {
		
		}
		
		#lii-logo {
		 BACKGROUND: url("/images/tl.gif") no-repeat top left;
		}
		
		#lii-logo, #collection-logo {
		 FLOAT: left;
		}
		
		#lii-logo a { 
		 MARGIN: 0;
		 PADDING: 0.2em 1em;
		 COLOR: #333;
		 FONT-SIZE: 1.2em;
		 LINE-HEIGHT: 2.7em;
		 TEXT-DECORATION: none;
		}
		
		#lii-logo a:hover, #lii-nav a.nav:hover, #collection-logo a:hover {
		 TEXT-DECORATION: none !important;
		 BACKGROUND: #f0f0f0;
		
		}
		DIV#lii-nav {
		 BACKGROUND: url("/images/tr.gif") no-repeat top right;
		 FLOAT: right;
		}
		
		#lii-nav UL, #lii-nav LI a {
		 BACKGROUND: #ccc;
		}
		
		#lii-nav UL, #collection-nav UL {
		 MARGIN: 0.6em 0 0 0;
		 LIST-STYLE-TYPE: none;
		}
		
		#lii-nav LI, #collection-nav LI { 
		 FLOAT: left; 
		 PADDING: 0.4em 0.1em 0 0.1em;
		 FONT-SIZE: 0.9em;
		 LINE-HEIGHT: 1.6em;
		}
		
		#lii-nav li a, #collection-nav LI a {
		 PADDING: 0.3em 0.9em;
		 TEXT-DECORATION: none;
		 COLOR: #333;
		}
		
	/* collection-level header */
	
		#collection-head {
		/* HEIGHT: 4.4em; */
		 HEIGHT: auto;
		 MIN-HEIGHT: 45px;
		 MAX-WIDTH: 68em;
		 MARGIN: 0 auto;
		 PADDING: 1em;
		 BACKGROUND: #f0f0f0;
		}
		
		#collectionchunk {
		 FLOAT: left;
		}
		
		#collection-logo a, #collection-logo a:visited {
		 MARGIN: 0.4em 2em 0 0;
		 PADDING: 0 0.5em;
		 FONT-SIZE: 2em;
		 TEXT-DECORATION: none !important;
		 COLOR: #333;
		}
		
		#collection-logo a:hover {
		 BACKGROUND: #ccc;
		}
		
		#collection-nav {
		 MARGIN: 0;
		 PADDING: 0;
		 MARGIN: 3.2em 0 0 0; /* should be about 4em for ff, 5em for opera */
		 PADDING: 0 1em;
		 FONT-SIZE: 0.9em;
		/* FLOAT: right;
		 MARGIN: 2em 1em; */
		}
		
		#collection-nav a {
		 MARGIN: 0;
		 PADDING: 0;
		 text-decoration: none;
		 PADDING: 0.2em 0.5em;
		}
		#collection-nav a:hover {
		 BACKGROUND: #ccc;
		}
		
		DIV#collection-pic {
		 FLOAT: right;
		 MARGIN: 0;
		 PADDING: 0 1.5%;
		}
		
		DIV#collection-pic IMG {
		 MARGIN: -1em 0;
		 PADDING: 1px;
		}
		
		#topNav FORM { 
		 VERTICAL-ALIGN: middle;
		 DISPLAY: inline;
		}
		
		#topNav INPUT {
		 VERTICAL-ALIGN: middle;
		}
		#topNav INPUT.txtbox {
		 MARGIN: 2px;
		}
		#topNav INPUT.btn {
		 MARGIN: 2px;
		 COLOR: #333;
		}
		
	/* middle */
	
		DIV#middle {
		 CLEAR: both;
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		 PADDING: 0;
		 BACKGROUND: #fff;
		}
		
		DIV#middle DIV.breathe {
		 MARGIN: 1ex 1%;
		}
		
		BR.clear, HR.clear {
		 CLEAR: both;
		 VISIBILITY: hidden;
		 MARGIN: 0;
		 PADDING: 0;
		}
		BR.clear {
		 FONT-SIZE: 1px;
		 LINE-HEIGHT: 1px; 
		}
		HR.clear {
		 BORDER: none;
		}
	
	/* footer styles */
		
		#footer { 
		 CLEAR: both; 
		 MAX-WIDTH: 70em;
		 MARGIN: 0 auto;
		 BACKGROUND: #ccc;
		 TEXT-ALIGN: center;
		}
		
		#footer .footerNav {
		 BACKGROUND: url("/images/br.gif") bottom right no-repeat;
		}
		
		#footer .footerNav DIV {
		 BACKGROUND: url("/images/bl.gif") bottom left no-repeat;
		}
		
		#footer UL { 
		 WIDTH: 25em;
		 MARGIN: 0 auto 1em;
		 PADDING: 1em 0;
		}
		
		#footer LI {
		 FLOAT: left;
		 PADDING: 0;
		 MARGIN: 0 0.5em;
		 COLOR: #333;
		 LIST-STYLE-TYPE: none;
		}
		#footer LI A {
		 TEXT-DECORATION: none;
		 COLOR: #333;
		 PADDING: 0.5em;
		}
		
		#footer LI A:hover {
		 TEXT-DECORATION: none;
		 BACKGROUND: #f0f0f0;
		}

/* generic */

		BODY {
		 FONT-FAMILY: verdana, arial, helvetica, sans-serif;
		 COLOR: #000000; 
		 BACKGROUND: #ffffff;
		 FONT-SIZE: 83%;
		}
		
		A:link {
		 COLOR: navy
		}
		
		P, OL, UL, LI, TD, TH, TR, DIV, DL, DD, DT, SPAN {
		 FONT-WEIGHT: normal;
		}
		
		LI {
		 display: list-item
		}
		
		STRONG, B {
		 FONT-WEIGHT: bold; 
		}
		
		EM, I {
		 FONT-STYLE: italic;
		}
		
		PRE {
		 FONT-FAMILY: courier, monospace;
		} 
		 
		H1, H2, H3, H4 { 
		 FONT-WEIGHT: normal !important; 
		} 
		
		H1 { 
		 FONT-SIZE: 1.6em; 
		}
		
		H2 { 
		 FONT-SIZE: 1.4em; 
		}
		
		H3 { 
		 FONT-SIZE: 1.3em; 
		}
		
		H4 { 
		 FONT-SIZE: 1.2em; 
		}
		
		H5 { 
		 FONT-SIZE: 1.1em; 
		}
		
		H6 { 
		 FONT-SIZE: 1.0em; 
		}

/* some retcons */
		P.contentIntro { PADDING: 2px 5px; FONT-SIZE: 0.9em; }
		.indented, .toc { MARGIN-LEFT: 5%; MARGIN-right: 5%; }
		.wasBlockquote { MARGIN-LEFT: 5%; MARGIN-right: 5%; }
		.allcaps { text-transform: uppercase; }
		.smallcaps { font-variant: small-caps; }
		.extraSpot 	 { FONT-SIZE: 2em; COLOR: yellow; FONT-WEIGHT: bold;}
		.emergency, .loud { COLOR: red; }
	
		/* for embedded definitions */
		A.defn_embed { text-decoration: none; }
		A.defn_embed:link:hover { text-decoration: underline; }

	
/* swish-e search result styles */

		SPAN.speling, .speling {
		 FONT-STYLE: italic;
		 color: red;
		}
		
		SPAN.highlight {
		 COLOR: red;
		 FONT-WEIGHT: bold;
		}
		
		BODY.hitlist H1.hitlist {
		 MARGIN-LEFT: 4%;
		 MARGIN-RIGHT: 4%;
		 FONT-SIZE: 1.8em;
		}
		
		BODY.hitlist H2 {
		 MARGIN-LEFT: 4%;
		 MARGIN-RIGHT: 4%;
		 PADDING-TOP: 1ex;
		 PADDING-LEFT: 1%;
		 PADDING-RIGHT: 1%;
		 BORDER-TOP: solid 2px #ccc;
		 FONT-SIZE: 1.7em
		} 
		
		BODY.hitlist H3 {
		 MARGIN-LEFT: 5%;
		 MARGIN-RIGHT: 5%;
		 PADDING-LEFT: 1%;
		 PADDING-RIGHT: 1%;
		 PADDING-TOP: 1ex;
		 BORDER-TOP: solid 1px #ccc;
 		}
		
		BODY.hitlist H4 {
		 COLOR: #000000;
		 MARGIN-LEFT: 6%;
		 MARGIN-RIGHT: 6%;
		 PADDING-LEFT: 1%;
		 PADDING-RIGHT: 1%;
		 FONT-WEIGHT: BOLD;	
		}
		
		BODY.hitlist DIV#middle DIV.swsearchbox {
     	MARGIN-LEFT: 6%;
		MARGIN-RIGHT: 6%;
		FONT-SIZE: 1.2em;
		}
		
		/* general rule */
		DIV.swsmallhelp { 
		MARGIN-LEFT: 7%;
     	FONT-STYLE: italic;
		FONT-SIZE: 0.7em;
		}
		/* but sometimes swsmallhelp is inside the swsearchbox div */
		BODY.hitlist DIV.swsearchbox DIV.swsmallhelp {
		MARGIN-LEFT: 2%;
		}
		
		H4.swsubtitle {
		 FONT-SIZE: 1em;	
		}
		
		BODY.hitlist P {
		 MARGIN-LEFT: 5%;	
		}
		BODY.hitlist P.featured {
		 MARGIN-LEFT: 8%;	
		 FONT-WEIGHT: bold;
		}
		
		BODY.hitlist UL.navline {
		 MARGIN-TOP: 1em;
		 COLOR: #ccc;
/*		 LIST-STYLE-TYPE: square;	*/
		 MARGIN-LEFT: 5%;
			}
	/* use to get rid of initial bullet
		UL.navline LI:first-child {
		 LIST-STYLE-TYPE: none;	
		}
	*/
		UL.navline LI {
		 COLOR: #999;
		 FLOAT: left;
		 PADDING-RIGHT: 1.5em;
		}

		DL.swhitlist { 
		 MARGIN-LEFT: 8%;
		 MARGIN-RIGHT: 8%;	
		}
		TABLE.swhitlist {
		 MARGIN-LEFT: 8%;
		 MARGIN-RIGHT: 8%;
		}
		TR.swhit { 
		vertical-align: top;
		}
		.swtitle { 
		vertical-align: top;
		}
		TD.swrank IMG {
		vertical-align: text-top;
		MARGIN: 0;
		PADDING: 0.5em;
		}
		TD.swtitle {
		 vertical-align: top;
		 MARGIN: 0;
		 PADDING: 0.5em;
		 }
		
		.swsearchparrot { 
		 FONT-SIZE: 1.1em;}
		 DL.swhitlist {
		 FONT-SIZE: 1em;	 
		 }
		 DL.swhitlist DT.popnames { 
		 MARGIN-TOP: 1em;
		 }
		 DL.swhitlist DD.popnames { 
		 }
		/* hitlist styles */
		
		UL.HITLIST{ MARGIN-LEFT: 10%; }
		DIV.HITLIST UL { MARGIN-LEFT: 10%; }
		LI.HITLIST { MARGIN-LEFT: 0; }
		DIV.HITLIST LI { MARGIN-LEFT: 0; }
		DIV.HITLIST P { MARGIN-LEFT: 0; }
	
/* 2005-06-16 */
/* separating columns from one another and from wide chunks */ 
		DIV.oneColumn, DIV.twoColumns, DIV.threeColumns {
		 CLEAR: both;
		 MARGIN: 1ex auto;
		}
		
		/* temporary */
		BODY.text H1 {
			MARGIN-LEFT: 1%;
		}
		BODY.text H2 {
			MARGIN-LEFT: 2%;
		}
		BODY.text H3 {
			MARGIN-LEFT: 4%
		}
		BODY.text UL, BODY.text OL, BODY.text DL, BODY.text FORM {
			MARGIN-LEFT: 10%;
			MARGIN-RIGHT: 10%;
		}
		
		BODY.text P, BODY.text H4, BODY.text H5, BODY.text H6 {
		 MARGIN-LEFT: 6%;
		 MARGIN-RIGHT: 6%;
		}
		/* temp - ie,nn ickyness */
		BODY#faq DIV#middle UL {
			MARGIN-LEFT: 6%;
			PADDING-LEFT: 5%;
		}
		BODY#faq DIV#middle UL UL {
			MARGIN-LEFT: 3%;
			PADDING-LEFT: 3%;
		}
		
		BODY.linklist DIV.linklist, BODY.text DIV.linklist {
		 CLEAR: both;
		 MARGIN-LEFT: auto;
		 MARGIN-RIGHT: auto;
		}
		BODY.linklist DIV.text {
		}
		
		/* end temporary */
		
		/* each of two equal columns */
		DIV.oneOfTwo, UL.oneOfTwo {
		 FLOAT: left;
		 WIDTH: 46%;
		 PADDING: 0 1%;	
		 BACKGROUND: #fff;	
		}
		
		/* each of three equal columns */
		DIV.oneOfThree, UL.oneOfThree {
		 FLOAT: left;
		 WIDTH: 31%;
		 PADDING: 0 1%;	
		 BACKGROUND: #fff;
		}
		
		.oneOfThree LI, .oneOfTwo LI {
		 LIST-STYLE-TYPE: none;
		}
		
		DIV#middle DIV.disclaimer {
		 WIDTH: 100%;
		 MARGIN: 0 auto;	
		}
			
		DIV.largerOfTwo, DIV#content {
		 FLOAT: left;
		 WIDTH: 66%;
		 MARGIN: 0;
		 PADDING: 0 1.5%;
		}
		
		.smallerOfTwo, #menu {
		 FLOAT: left;
		 WIDTH: 27%;
		 MARGIN: 0;
		 PADDING: 0 1.5%;
		}
		
		DIV.paris {
		 FLOAT: right;
		}
		DIV.paris, DIV.paris A, DIV.paris IMG, DIV.paris A IMG 
	    { 
		 MARGIN: 0;
		 PADDING: 0;	
		}

		
		DIV#middle DIV.smallerOfTwo, DIV#menu {
		 BACKGROUND: #f0f0f0
		}
	
		
		.smallOneOfThree {
		 FLOAT: left;
		 WIDTH: 20%;
		 PADDING: 0 2%;
		} 
		
		DIV#middle DIV.smallOneOfThree {
		 BACKGROUND: #f0f0f0; 
		}
		
		.largeOneOfThree {
		 FLOAT: left;
		 WIDTH: 47%;
		 PADDING: 0 2%
		}

 
/* 2005-10-06 headers indicate "you are here" */
		BODY.homepg a#homelink, BODY.hitlist a#searchlink, BODY.tellmore a#telllink, BODY.donors a#donorlink, BODY.faq a#faqlink, BODY#faq a#faqlink, BODY#faq a#aboutlink, BODY#about a#aboutlink, BODY#subscribe a#subscribelink, BODY#main a#homelink {
		 FONT-WEIGHT: bold;
		}
		
		BODY#liihome DIV#lii-nav A#liihomelink,
		BODY#liisearch DIV#lii-nav #liisearchlink,
		BODY#liisitemap DIV#lii-nav A#liisitemaplink,
		BODY#liidonate DIV#lii-nav A#liidonatelink {
		 FONT-WEIGHT: bold;
		}

/* hack, hackity hack... don't you just adore IE? */

