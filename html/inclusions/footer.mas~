<%init>


use constant PUBNUMBER => "ca-pub-6351434937294517";

 my $uri = $r -> uri();
 my $collectionname = '';
 my $collectiontitle = '';
 my $collectionnav = '';
 my $toolboxtitle = '';
 my $toolboxcontents = '';
 my $topname = '';
 my $midname = '';
 my $bottomname = '';
 my @collectionstuff = undef;
 if ($uri =~ /^.*uscode\/.*$/){
  $collectionname = "uscode";
  $collectiontitle = "U.S. Code";
  $toolboxtitle = "U.S. Code";
  $toolboxcontents = '';
  $topname = "USC_ROC_TopRight_300x250";
  $midname = "USC_ROC_MidRight_300x250";
  $bottomname = "USC_ROC_Footer_728x90";
  }
 elsif ($uri =~ /^.*supct\/.*$/) {
  $collectionname = "supct";
  $collectiontitle = "U.S. Supreme Court";
  $toolboxtitle = "Supreme Court";
  $toolboxcontents = '';
  $topname = "SUP_ROC_TopRight_300x250";
  $midname = "SUP_ROC_MidRight_300x250";
  $bottomname = "SUP_ROC_Footer_728x90";
 }
 elsif ($uri =~ /^.*frcp\/.*$/) {
  $collectionname = "frcp";
  $collectiontitle = "Federal Rules of Civil Procedure";
  $toolboxtitle = "Civil Procedure";
  $toolboxcontents = '';
  $topname = "Rules_Civil_Procedure_ROC_TopRight_300x250";
  $midname = "Rules_Civil_Procedure_ROC_MidRight_300x250";
  $bottomname = "Rules_Civil_Procedure_ROC_Footer_728x90";
 }
 elsif ($uri =~ /^.*frcrmp\/.*$/) {
  $collectionname = "frcrmp";
  $collectiontitle = "Federal Rules of Criminal Procedure";
  $toolboxtitle = "Criminal Procedure";
  $toolboxcontents = '';
  $topname = "Rules_Criminal_Procedure_ROC_TopRight_300x250";
  $midname = "Rules_Criminal_Procedure_ROC_MidRight_300x250";
  $bottomname = "Rules_Criminal_Procedure_ROC_Footer_728x90";
 }
  elsif ($uri =~ /^.*fre\/.*$/) {
  $collectionname = "fre";
  $collectiontitle = "Federal Rules of Evidence";
  $toolboxtitle = "Evidence";
  $toolboxcontents = '';
  $topname = "Rules_Evidence_ROC_TopRight_300x250";
  $midname = "Rules_Evidence_ROC_MidRight_300x250";
  $bottomname = "Rules_Evidence_ROC_Footer_728x90";
 }
  elsif ($uri =~ /^.*frap\/.*$/) {
  $collectionname = "frap";
  $collectiontitle = "Federal Rules of Appellate Procedure";
  $toolboxtitle = "Appellate Procedure";
  $toolboxcontents = '';
  $topname = "Rules_Appellate_ROC_TopRight_300x250";
  $midname = "Rules_Appellate_ROC_MidRight_300x250";
  $bottomname = "Rules_Appellate_ROC_Footer_728x90";
 }
  elsif ($uri =~ /^.*frbp\/.*$/) {
  $collectionname = "frbp";
  $collectiontitle = "Federal Rules of Bankruptcy Procedure";
  $toolboxtitle = "Bankruptcy";
  $toolboxcontents = '';
  $topname = "Rules_Bankruptcy_ROC_TopRight_300x250";
  $midname = "Rules_Bankruptcy_ROC_MidRight_300x250";
  $bottomname = "Rules_Bankruptcy_ROC_Footer_728x90";
 }
  elsif ($uri =~ /^.*ucc\/.*$/) {
  $collectionname = "ucc";
  $collectiontitle = "Uniform Commercial Code";
  $toolboxtitle = "UCC";
  $toolboxcontents = '';
  $topname = "UCC_ROC_TopRight_300x250"; 
  $midname = "";
  $bottomname = "";
}
  elsif ($uri =~ /^.*anncon\/.*$/) {
	  $collectionname = "anncon";
	  $collectiontitle = "CRS Annotated Constitution";
	  $toolboxtitle = "Constitution";
	  $toolboxcontents = '';
	  $topname = "LII_DEFAULT_TopRight_300x250";
	  $midname = "LII_DEFAULT_MidRight_300x250";
	  $bottomname="LII_DEFAULT_Footer_728x90";
	}
  else {
  $toolboxtitle = "";
  $collectionname = "";
  $collectiontitle = "";
  $toolboxtitle = "";
  $toolboxcontents = '';
  $topname = "LII_DEFAULT_TopRight_300x250";
  $midname = "LII_DEFAULT_MidRight_300x250";
  $bottomname="LII_DEFAULT_Footer_728x90";
 }


</%init>
